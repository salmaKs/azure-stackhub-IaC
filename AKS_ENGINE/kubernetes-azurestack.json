      "serviceCidr": "10.240.0.0/16",
      "dnsServiceIP": "10.240.0.10",
      "controllerManagerConfig": {
        "--node-monitor-grace-period": "5m",
        "--pod-eviction-timeout": "5m",
        "--route-reconciliation-period": "1m"
      }
    }
  },

"customCloudProfile": {
    "portalURL": "https://portal.dc2.xpressazure",
